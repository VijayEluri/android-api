<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.snaptic.WODWhacker"
      android:versionCode="1"
      android:versionName="1.0">
      
      <uses-permission android:name="android.permission.INTERNET" />
      <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
      
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".WorkoutEditor"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

		<activity
			android:name=".ExerciseProperties"
			android:label="ExcerciseProperties">
			
			<intent-filter>
			<action android:name="com.android.intent.action.EXERCISE_PROPERTIES" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		
				<activity
			android:name=".SignInActivity"
			android:label="@string/snaptic_sign_in_title">
			
			<intent-filter>
				<action android:name="com.snaptic.WODWhacker.intent.action.SIGN_IN" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
    </application>



</manifest> 